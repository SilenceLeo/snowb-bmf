    "@testing-library/user-event": "^12.1.10",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/color": "^3.0.1",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
